<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="rectangle">
            <corners
                android:radius="10dp"
                />
            <padding
                android:bottom="1dp"
                android:left="1dp"
                android:right="1dp"
                android:top="1dp"/>
            <stroke
                android:width="1dp"
                android:color="@android:color/darker_gray"/>
        </shape>
    </item>
    <item>
        <shape
            android:shape="rectangle"
            >
            <corners
                android:radius="10dp"
                />
            <solid
                android:color="@color/white_button_text"/>

            <size
                android:width="120dp"
                android:height="120dp"/>
        </shape>

    </item>
</layer-list>

